RewriteEngine on

# Redirection HTTPS
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^fidelilium\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.fidelilium\.com$
RewriteRule ^/?$ "https\:\/\/fidelilium\.com\/" [R=301,L]

# Inclure la configuration CSP
#php_value auto_prepend_file "/csp_config.php"









